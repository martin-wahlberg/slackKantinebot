{
	"compilerOptions": {
		"module": "commonjs",
		"esModuleInterop": true,
		"target": "es5",
		"noImplicitAny": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "built",
		"baseUrl": ".",
		"paths": {
			"*": ["node_modules/*", "src/types/*"]
		},
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true
	},
	"include": ["src/**/*"]
}
